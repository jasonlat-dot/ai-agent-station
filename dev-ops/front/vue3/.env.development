# 开发环境配置文件
# 用于Vue CLI开发时的环境变量设置

# 应用环境
NODE_ENV=development

# Vue CLI 应用配置 (VUE_APP_ 前缀)
VUE_APP_API_BASE_URL=http://localhost:8888
VUE_APP_API_TIMEOUT=30000
VUE_APP_TITLE=AI Auto Agent By Jasonlat (Dev)
VUE_APP_VERSION=1.0.0-dev
VUE_APP_DEBUG=true

# 网络配置
VUE_APP_NETWORK_TIMEOUT=30000
VUE_APP_NETWORK_RETRY=3

# 安全配置
VUE_APP_ENABLE_HTTPS=false
VUE_APP_SECURE_COOKIES=false

# 构建配置
VUE_APP_BUILD_SOURCEMAP=true
VUE_APP_BUILD_MINIFY=false

# 部署配置
VUE_APP_PUBLIC_PATH=/
VUE_APP_OUTPUT_DIR=dist