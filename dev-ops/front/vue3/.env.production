# 生产环境配置文件
# 用于Vue CLI构建时的环境变量设置

# 应用环境
NODE_ENV=production

# Vue CLI 应用配置 (VUE_APP_ 前缀)
VUE_APP_API_BASE_URL=https://autoAgent.jasonlat.com
VUE_APP_API_TIMEOUT=30000
VUE_APP_TITLE=AI Auto Agent By Jasonlat
VUE_APP_VERSION=1.0.0
VUE_APP_DEBUG=false

# 网络配置
VUE_APP_NETWORK_TIMEOUT=30000
VUE_APP_NETWORK_RETRY=3

# 安全配置
VUE_APP_ENABLE_HTTPS=false
VUE_APP_SECURE_COOKIES=true

# 构建配置
VUE_APP_BUILD_SOURCEMAP=false
VUE_APP_BUILD_MINIFY=true

# 部署配置
VUE_APP_PUBLIC_PATH=/
VUE_APP_OUTPUT_DIR=dist