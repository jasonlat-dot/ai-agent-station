# 生产环境配置文件
# 用于Docker部署时的环境变量设置

# 应用环境
NODE_ENV=production
VITE_MODE=production

# API配置
VITE_API_BASE_URL=https://autoAgent.jasonlat.com
VITE_API_TIMEOUT=30000

# 应用配置
VITE_APP_TITLE=AI Auto Agent By Jasonlat
VITE_APP_VERSION=1.0.0

# 调试配置
VITE_DEBUG=false

# 构建配置
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true

# 网络配置
VITE_NETWORK_TIMEOUT=30000
VITE_NETWORK_RETRY=3

# 安全配置
VITE_ENABLE_HTTPS=false
VITE_SECURE_COOKIES=true